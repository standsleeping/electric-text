      "name": "Debug web server",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "electric_text.web.server:app",
        "--reload",
      ],
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "VIRTUAL_ENV": "${workspaceFolder}/.venv",
